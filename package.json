{
  "name": "pyxom",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "npm run clean && next build",
    "build:fast": "npm run clean && next build --no-lint",
    "build:production": "npm run clean:all && npm run build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "test:e2e": "playwright test",
    "test:e2e:clean": "playwright test",
    "dev:e2e": "concurrently \"npm run dev\" \"npm run test:e2e -- --watch\"",
    "test": "npm run test:e2e",
    "clean": "rm -rf .next",
    "clean:all": "rm -rf .next && rm -rf out && rm -rf node_modules/.cache",
    "analyze": "cross-env ANALYZE=true npm run build"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "critters": "^0.0.23",
    "next": "^14.2.28",
    "pyodide": "^0.27.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^8.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "22.15.3",
    "@types/react": "19.1.2",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.4.24",
    "start-server-and-test": "^1.13.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.3.3",
    "webpack": "^5.0.0"
  }
}
